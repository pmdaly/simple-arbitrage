{
  "name": "@flashbots/simple-arbitrage",
  "version": "0.0.1",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "yarn tsc",
    "chain": "hardhat typechain",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "console": "hardhat console",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.ts\"",
    "deploy": "hardhat run scripts/deploy.ts",
    "deploy:network": "hardhat run scripts/deploy.ts --network",
    "lint": "yarn run lint:sol && yarn run lint:ts && yarn run prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "node": "hardhat node",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc \"**/*.{js,json,md,sol,ts}\"",
    "prepare": "yarn run build",
    "start": "yarn ts-node --project tsconfig.json src/index.ts",
    "test": "hardhat test"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.2.0",
    "@types/fs-extra": "^9.0.12",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "chai": "^4.3.4",
    "eslint": "^7.15.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.0.23",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.5.0",
    "hardhat-gas-reporter": "^1.0.4",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "shelljs": "^0.8.4",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "typechain": "^5.1.2"
  },
  "peerDependencies": {
    "ethers": "^5.0.23"
  },
  "dependencies": {
    "@flashbots/ethers-provider-bundle": "^0.3.1",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  }
}
